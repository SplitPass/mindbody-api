{
  "compilerOptions": {
    "allowUnreachableCode": false,
    "allowUnusedLabels": false,
    "baseUrl": "./src",
    "importsNotUsedAsValues": "error",
    "moduleResolution": "node",
    "paths": {
      "$http/*": ["http/*"],
      "$mindbody/*": ["mindbody/*"],
      "$types": ["types.ts"],
      "$*": ["*"]
    },
    "strict": true,
    "target": "ES2019",
    "types": ["node"]
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}
