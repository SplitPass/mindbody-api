{
  "name": "mindbody",
  "version": "0.1.0",
  "description": "Type safe library for interacting with Mindbody's Public API (v6) and Webhooks",
  "author": "Kyle Surowiec <kyle@splitpass.com>",
  "contributors": [
    "Ben ElDarragi <ben@splitpass.com>"
  ],
  "repository": "github:splitpass/mindbody-api",
  "license": "MIT",
  "keywords": [
    "mindbody",
    "mindbody-api",
    "booking",
    "fitness",
    "node"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "engines": {
    "node": ">=12.4",
    "yarn": "Please use pnpm",
    "npm": "Please use pnpm"
  },
  "scripts": {
    "build": "tsup",
    "example": "pnpm ts-node --esm examples/example.ts",
    "lint": "pnpm tsc --noEmit --skipLibCheck & eslint \"./src/**/*.ts\""
  },
  "dependencies": {
    "axios": "1.1.2"
  },
  "devDependencies": {
    "@types/node": "^18.8.3",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "eslint": "8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "prettier": "^2.7.1",
    "ts-essentials": "^9.3.0",
    "ts-node": "^10.9.1",
    "tsup": "^6.2.3",
    "typescript": "^4.8.4"
  }
}